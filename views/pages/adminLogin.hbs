 <!DOCTYPE html>
 <html lang="en">
 <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>ADMIN PAGE</title>
    <link rel="stylesheet" href="/styles/adminLogin.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" integrity="sha512-iecdLmaskl7CVkqkXNQ/ZH/XLlvWZOJyj7Yy7tcenmpD1ypASozpmT/E0iPtmFIB46ZmdtAc9eNBvH0H/ZpiBw==" crossorigin="anonymous" referrerpolicy="no-referrer" />
 </head>
 <body>
    <form action="/admin" method="POST">
        <section class="section1">
            <input type="text" name="userName" placeholder="USERNAME">
            <div class="psswd">
                <input type="password" name="psswd" id="passwordField" placeholder="Enter Password..">
                <button onclick="togglePasswordVisibility(event)" class="eye"><i class="fa-regular fa-eye-slash"></i></button>
            </div>
        </section>
        <section class="section2">
            <div class="logo-section">
                <div class="logo">
                    <img src="data:image/png;base64," alt="">
                </div>
                <h4>PetsWorld</h4>
            </div>
            <h2> WELCOME ADMIN</h2>
            <button type="submit">Welcome</button>
        </section>
    </form>
    <script>
        let eye = document.querySelector(".eye");
        function togglePasswordVisibility() {
            event.preventDefault();
            const passwordField = document.getElementById('passwordField');
            if (passwordField.type === 'password') {
                passwordField.type = 'text';
                eye.innerHTML='<i class="fa-regular fa-eye">';
            } else {
                passwordField.type = 'password';
                eye.innerHTML='<i class="fa-regular fa-eye-slash"></i>';
            }
        }
    </script>
 </body>
 </html>